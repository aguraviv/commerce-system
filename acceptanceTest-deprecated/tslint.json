{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-any": false,
        "member-access": false,
        "ordered-imports":false,
        "max-line-length": false,
        "eofline": false,
        "no-trailing-whitespace": false,
        "comment-format": false,
        "quotemark": false,
        "no-console": false,
        "one-line": false,
        "no-consecutive-blank-lines": false,
        "curly": false,
        "no-debugger":false

        
    },
    "rulesDirectory": []
}